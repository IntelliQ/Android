<queue-entries>
  <total-storage-limit>50M</total-storage-limit>
  <queue>
    <name>parser</name>
    <rate>6/m</rate>
    <bucket-size>1</bucket-size>
    <max-concurrent-requests>1</max-concurrent-requests>
    <retry-parameters>
      <task-retry-limit>5</task-retry-limit>
      <task-age-limit>2d</task-age-limit>
    </retry-parameters>
  </queue>
</queue-entries>